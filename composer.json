{
    "name": "hellomayaagency/enso-mailer",
    "description": "Enso Mailer package",
    "keywords": [
        "hellomayaagency",
        "laravel",
        "enso-mailer"
    ],
    "homepage": "https://github.com/hellomayaagency/enso-mailer",
    "license": "MIT",
    "authors": [
        {
            "name": "Jake Gully",
            "email": "Jake@maya.agency",
            "role": "Developer"
        },
        {
            "name": "Andrew Ellender",
            "email": "andrew@maya.agency",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.4",
        "illuminate/contracts": "^8.0",
        "spatie/laravel-package-tools": "^1.9.2"
    },
    "require-dev": {
        "nunomaduro/collision": "^5.0",
        "nunomaduro/larastan": "^1.0",
        "orchestra/testbench": "^6.24",
        "phpstan/extension-installer": "^1.1",
        "phpstan/phpstan-deprecation-rules": "^1.0",
        "phpstan/phpstan-phpunit": "^1.0",
        "phpunit/phpunit": "^8.5",
        "spatie/laravel-ray": "^1.26"
    },
    "autoload": {
        "psr-4": {
            "Hellomayaagency\\EnsoMailer\\": "src",
            "Hellomayaagency\\EnsoMailer\\Database\\Factories\\": "database/factories"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Hellomayaagency\\EnsoMailer\\Tests\\": "tests"
        }
    },
    "scripts": {
        "analyse": "vendor/bin/phpstan analyse",
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "phpstan/extension-installer": true
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Hellomayaagency\\EnsoMailer\\EnsoMailerServiceProvider"
            ],
            "aliases": {
                "EnsoMailer": "Hellomayaagency\\EnsoMailer\\Facades\\EnsoMailer"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
